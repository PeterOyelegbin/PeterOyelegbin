{
  "version": 2,
  "builds": [
    {
      "src": "peteroyelegbin/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "pythonVersion": "3.11" 
      }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "peteroyelegbin/wsgi.py"
    }
  ]
}
